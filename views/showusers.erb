<table class="table table-striped">
	<thead>
		<tr>
			<th>Имя</th>
			<th>Дата и время</th>
			<th>Парикмахер</th>
			<th>Цвет</th>
			<th>Телефон</th>
		</tr>
	</thead>

	<tbody>
		<% @results.each do |row| %>
			<tr>
				<td><%= row['name'] %></td>
				<td><%= row['datestamp'] %></td>
				<td><%= row['barber'] %></td>
				<td><%= row['color'] %></td>
				<td><%= row['phone'] %></td>
			</tr>
		<% end %>
	</tbody>
</table>